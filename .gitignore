# =====================================================
vendor/
composer.phar
composer.lock
.env
*.cache

# Archivos de configuración local (no subir credenciales)
backend-php/config/database.local.php

# =====================================================
# MYSQL / DATABASE
# =====================================================
*.sql.backup
*.sql~

# =====================================================
# ANDROID
# =====================================================

# Built application files
*.apk
*.ap_
*.aab

# Files for the ART/Dalvik VM
*.dex

# Java class files
*.class

# Generated files
bin/
gen/
out/
release/

# Gradle files
.gradle/
build/
*/build/

# Local configuration file (sdk path, etc)
local.properties

# Proguard folder generated by Eclipse
proguard/

# Log Files
*.log

# Android Studio Navigation editor temp files
.navigation/

# Android Studio captures folder
captures/

